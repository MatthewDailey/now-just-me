{
  "manifest_version": 2,
  "name": "Now Just Me",
  "version": "1.0.0",
  "description": "Hide feeds on LinkedIn and Twitter",
  "icons": {
  },
  "content_scripts": [
    {
      "matches": ["*://www.linkedin.com/*", "*://linkedin.com/*"],
      "js": ["dist/linkedin.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["*://twitter.com/*", "*://x.com/*"],
      "js": ["dist/twitter.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": []
}